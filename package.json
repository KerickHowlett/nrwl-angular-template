{
    "name": "nrwl-angular-monorepo-template",
    "version": "1.0.0",
    "description": "Insert repo description of monorepo here.",
    "license": "AIC",
    "private": true,
    "engines": {
        "node": "~14.16.1",
        "npm": "~7.12.0"
    },
    "os": [
        "linux",
        "darwin"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/KerickHowlett/[repo-uri].git"
    },
    "author": "Kerick Howlett",
    "keywords": [
        "Angular",
        "NRWL",
        "Nx",
        "Monorepo"
    ],
    "config": {
        "commitizen": {
            "path": "node_modules/cz-customizable"
        }
    },
    "scripts": {
        "affected": "nx affected",
        "affected:apps": "nx affected:apps",
        "affected:build": "nx affected:build --uncommitted --parallel",
        "affected:dep-graph": "nx affected:dep-graph",
        "affected:e2e": "nx affected:e2e --uncommitted --parallel",
        "affected:libs": "nx affected:libs",
        "affected:lint": "nx affected:lint --uncommitted --parallel",
        "affected:lint:fix": "nx affected:lint --fix --uncommitted --parallel",
        "affected:style": "nx affected:stylelint",
        "affected:style:fix": "nx affected:stylelint  --fix --uncommitted --parallel",
        "affected:test": "nx affected:test --uncommitted --parallel --code-coverage",
        "affected:test:output": "nx affected:test --uncommitted --parallel --code-coverage --json --outputFile=./test-results/.jest-test-results.json",
        "affected:test:watch": "nx affected:test --uncommitted --parallel --code-coverage --watch",
        "affected:test:watch:output": "nx affected:test --uncommitted --parallel --code-coverage --watch --json --outputFile=./test-results/.jest-test-results.json",
        "build": "ng build",
        "build:prod": "ng build -c=production",
        "clean": "rimraf node_modules yarn.lock",
        "commit": "git-cz",
        "commit:check": "node ./.config/commitlint.config.js",
        "dep-graph": "nx dep-graph",
        "e2e": "ng e2e",
        "format": "nx format:write",
        "format:check": "nx format:check",
        "format:package": "format-package -w",
        "format:write": "nx format:write",
        "help": "nx help",
        "lint": "nx lint",
        "lint-staged": "lint-staged --config='./.config/.lintstagedrc.json'",
        "lint:all": "nx run-many --all --target=test --parallel",
        "lint:all:fix": "nx run-many --all --target=test --parallel --fix",
        "lint:ci": "nx lint --quiet",
        "lint:fix": "yarn lint --fix=true",
        "lint:global": "run-p -c lint:workspace lint",
        "lint:styles": "stylelint \"apps/**/*.scss\" && stylelint \"libs/**/*.scss\"",
        "lint:styles:fix": "stylelint \"apps/**/*.scss\" --fix && stylelint \"libs/**/*.scss\" --fix",
        "lint:workspace": "nx workspace-lint",
        "ng": "nx",
        "nx": "nx",
        "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
        "prepare": "husky install",
        "prereset:dependencies": "yarn clean",
        "reset:dependencies": "yarn install",
        "start": "ng serve",
        "stylelint": "stylelint",
        "test": "nx test --code-coverage --runInBand",
        "test:all": "nx run-many --all --target=test --parallel --code-coverage --runInBand",
        "test:output": "nx --target=test --parallel --code-coverage --runInBand --json --outputFile=./test-results/.jest-test-results.json",
        "test:watch": "nx test --code-coverage --runInBand --watchAll",
        "test:watch:output": "nx test --code-coverage --runInBand --watchAll --json --outputFile=./test-results/.jest-test-results.json",
        "update": "nx migrate latest",
        "workspace-generator": "nx workspace-generator"
    },
    "dependencies": {
        "@angular/animations": "12.0.0",
        "@angular/common": "12.0.0",
        "@angular/compiler": "12.0.0",
        "@angular/core": "12.0.0",
        "@angular/forms": "12.0.0",
        "@angular/platform-browser": "12.0.0",
        "@angular/platform-browser-dynamic": "12.0.0",
        "@angular/router": "12.0.0",
        "@nrwl/angular": "12.3.5",
        "rxjs": "~6.6.0",
        "tslib": "^2.0.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "12.0.0",
        "@angular-eslint/eslint-plugin": "~12.0.0",
        "@angular-eslint/eslint-plugin-template": "~12.0.0",
        "@angular-eslint/template-parser": "~12.0.0",
        "@angular/cli": "12.0.0",
        "@angular/compiler-cli": "12.0.0",
        "@angular/language-service": "12.0.0",
        "@nrwl/cli": "12.3.5",
        "@nrwl/cypress": "12.3.5",
        "@nrwl/eslint-plugin-nx": "12.3.5",
        "@nrwl/jest": "12.3.5",
        "@nrwl/linter": "12.3.5",
        "@nrwl/tao": "12.3.5",
        "@nrwl/workspace": "12.3.5",
        "@types/jest": "26.0.8",
        "@types/node": "14.14.33",
        "@typescript-eslint/eslint-plugin": "4.19.0",
        "@typescript-eslint/parser": "4.19.0",
        "commitizen": "^4.2.4",
        "cypress": "^7.3.0",
        "cz-conventional-changelog": "^3.3.0",
        "cz-customizable": "^6.3.0",
        "dotenv": "8.2.0",
        "eslint": "7.22.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-cypress": "^2.11.3",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jest": "^24.3.6",
        "eslint-plugin-sort": "^1.5.0",
        "format-package": "^6.1.0",
        "husky": "^6.0.0",
        "jest": "26.2.2",
        "jest-preset-angular": "8.4.0",
        "lint-staged": "^11.0.0",
        "ng-mocks": "^12.0.2",
        "npm-run-all": "^4.1.5",
        "nx-stylelint": "^12.1.2",
        "prettier": "2.2.1",
        "rimraf": "^3.0.2",
        "stylelint": "^13.13.1",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-standard": "^22.0.0",
        "stylelint-scss": "^3.19.0",
        "ts-jest": "26.5.5",
        "ts-node": "~9.1.1",
        "typescript": "~4.2.4"
    }
}
